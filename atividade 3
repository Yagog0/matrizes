#include <iostream>
#include <iomanip>
#include <chrono>
#include <thread>
#include <ctime>
#include <unistd.h>
#define TAM 5
using namespace std;

int main() {
    int matriz [TAM] [TAM], linha, coluna;
    cout << "digite 25 valores para prencher uma matriz 5x5: \n";
    for (linha = 0; linha < TAM; linha++) {
        for (coluna = 0; coluna < TAM; coluna++) {
            cin >> matriz[linha][coluna];

        }
    }
    cout << "matriz digitada: \n";
    for (linha = 0; linha < TAM; linha++) {
        for (coluna = 0; coluna < TAM; coluna++) {
            cout << setw(4) << matriz[linha][coluna];
        }
        cout << endl;
    }
    return 0;
}
